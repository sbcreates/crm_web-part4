
<p class='contact_total'>There are <span>x</span> contacts</p>

<ul class='contact_list'>
  <% @contacts.each do |contact| %>
  <li><a href="/contacts/<%= contact[:id] %>"><%= contact[:first_name] %> <%= contact[:last_name] %> (<%= contact[:email] %>)</a>
    <h6 class='edit_delete_contact'><a href="/contacts/<%= contact[:id] %>/edit">Edit</a> | <a href="#">Delete</a></h6>
    <% end %>
  </li>
</ul>
